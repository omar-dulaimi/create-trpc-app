{
  "name": "create-trpc-appx",
  "version": "0.0.2",
  "type": "module",
  "keywords": [
    "trpc",
    "create-trpc-app"
  ],
  "description": "Create tRPC-powered apps with one command",
  "repository": "https://github.com/omar-dulaimi/create-trpc-app",
  "author": {
    "name": "Omar Dulaimi",
    "url": "https://github.com/omar-dulaimi"
  },
  "license": "MIT",
  "bin": {
    "create-trpc-appx": "./dist/index.js"
  },
  "scripts": {
    "build": "rimraf dist && tsup index.ts helpers/run.ts --format esm --dts && cp -r templates dist",
    "doctor": "node dist/index.js doctor",
    "test": "npm run build && node tests/run.test.mjs && node tests/pm.test.mjs",
    "release": "semantic-release",
    "package:publish": "./package.sh && cd package && npm publish",
    "test:smoke": "node tests/smoke.mjs",
    "lint": "eslint . --ext .ts,.js --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^12.0.1",
    "async-retry": "1.3.3",
    "chalk": "^4.1.2",
    "commander": "12.0.0",
    "cpy": "8.1.2",
    "cross-spawn": "7.0.3",
    "got": "^14.2.1",
    "ora": "^8.2.0",
    "prompts": "2.4.2",
    "rimraf": "5.0.5",
    "semver": "^7.6.3",
    "tar": "7.0.1",
    "typescript": "5.4.5",
    "unzipper": "^0.12.3",
    "update-check": "1.5.4",
    "validate-npm-package-name": "5.0.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.0",
    "@semantic-release/github": "^9.2.4",
    "@semantic-release/npm": "^11.0.0",
    "@semantic-release/release-notes-generator": "^12.0.0",
    "@types/async-retry": "1.4.8",
    "@types/cross-spawn": "^6.0.6",
    "@types/got": "^9.6.12",
    "@types/node": "20.12.7",
    "@types/prompts": "2.4.9",
    "@types/semver": "^7.5.8",
    "@types/tar": "6.1.12",
    "@types/validate-npm-package-name": "4.0.2",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^15.11.0",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "semantic-release": "^23.0.0",
    "tsup": "^7.2.0",
    "tsx": "^4.7.0"
  }
}
