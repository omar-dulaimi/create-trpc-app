{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
  "lib": ["ESNext", "DOM", "DOM.Iterable"],
  "moduleResolution": "bundler",
  "resolveJsonModule": true,
  "isolatedModules": true,
  "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
